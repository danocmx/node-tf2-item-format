{
  "name": "tf2-item-format",
  "version": "5.9.26",
  "description": "Format's TF2 items like backpack.tf does",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./static": "./dist/static.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "static": [
        "./dist/static.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "mocha ./test",
    "test:coverage": "nyc npm run test",
    "build": "npx tsc",
    "format": "npx prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danocmx/node-tf2-item-format.git"
  },
  "keywords": [
    "tf2",
    "backpacktf"
  ],
  "author": "danocmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danocmx/node-tf2-item-format/issues"
  },
  "homepage": "https://github.com/danocmx/node-tf2-item-format#readme",
  "optionalDependencies": {
    "tf2-static-schema": "^1.88.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "chai": "^5.2.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^11.0.1",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
